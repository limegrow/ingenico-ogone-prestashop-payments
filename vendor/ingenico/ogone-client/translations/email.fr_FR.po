msgid "admin_authorization.subject"
msgstr "Paiement autorisé. Saisie manuelle requise."

msgid "admin_authorization.dear_manager"
msgstr "Cher %name% gestionnaire"

msgid "admin_authorization.text1"
msgstr "Le paiement de la <a href=\"%url%\">commande %order_id%</a> a été validé et requiert une saisie manuelle."

msgid "admin_authorization.text2"
msgstr "Pour ce faire, veuillez <a href=\"%url%\">ouvrir les détails de la commande</a> et cliquer sur le bouton \"Capture\"."

msgid "admin_authorization.regards"
msgstr "Meilleures salutations"

msgid "admin_authorization.team"
msgstr "L’équipe %platform_name%"

msgid "admin_order_paid.subject"
msgstr "Statut de la commande modifié"

msgid "admin_order_paid.dear"
msgstr "Cher/Chère %name%"

msgid "admin_order_paid.text1"
msgstr "La commande <a href=\"%url%\">%order_id%</a> a été payée par votre client et n’a plus le statut Annulé."

msgid "admin_order_paid.text2"
msgstr "La commande [%url%] %order_id% a été payée par votre client et n’a plus le statut Annulé."

msgid "admin_order_paid.text3"
msgstr "Veuillez évaluer votre stock et vérifier si vous devez rembourser ou saisir le paiement."

msgid "admin_order_paid.regards"
msgstr "Meilleures salutations"

msgid "admin_order_paid.team"
msgstr "L’équipe %platform_name%"

msgid "admin_refund_failed.subject"
msgstr "Remboursement impossible"

msgid "admin_refund_failed.hello"
msgstr "Bonjour,"

msgid "admin_refund_failed.text1"
msgstr "Nous n’avons pas pu effectuer de remboursement en faveur de votre client pour la commande %order_id%."

msgid "admin_refund_failed.text2"
msgstr "Ses moyens de paiement ont probablement changé ou ne prennent pas en charge les remboursements."

msgid "admin_refund_failed.text3"
msgstr "Veuillez contacter votre client pour pouvoir procéder au remboursement."

msgid "admin_refund_failed.regards"
msgstr "Meilleures salutations"

msgid "admin_refund_failed.team"
msgstr "L’équipe %platform_name%"

