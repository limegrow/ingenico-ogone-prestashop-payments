msgid "reminder.text1"
msgstr "Nous constatons que vous n’avez pas procédé au paiement de votre commande du %shop_name%"

msgid "reminder.text2"
msgstr "Voici le contenu de votre commande"

msgid "reminder.item"
msgstr "Article"

msgid "reminder.price"
msgstr "Prix"

msgid "reminder.total"
msgstr "Montant total"

msgid "reminder.text3"
